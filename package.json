{
  "name": "preact-interactively",
  "version": "0.0.1",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "vite build",
    "serve": "export NODE_ENV=production && vite preview --port 3006",
    "start": "export NODE_ENV=development && rimraf dev-dist && cross-env vite --force --port 3006",
    "lint": "eslint src",
    "test": "jest ./tests"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "@preact/preset-vite": "^2.6.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.2",
    "@unocss/preset-icons": "^0.45.18",
    "@vitejs/plugin-legacy": "^4.1.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.4",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "https-localhost": "^4.7.1",
    "path": "^0.12.7",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-compression": "^0.5.1"
  },
  "dependencies": {
    "@preact/signals": "1.2.1",
    "@vx/axis": "0.0.199",
    "@vx/grid": "0.0.199",
    "@vx/scale": "0.0.199",
    "@vx/shape": "0.0.199",
    "chart.js": "4.4.0",
    "d3": "7.8.5",
    "json-loader": "0.5.7",
    "preact": "10.18.1"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/src/tests/__mocks__/browserMocks.js"
    ],
    "testURL": "http://localhost:3006",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^./style$": "identity-obj-proxy",
      "^preact$": "<rootDir>/node_modules/preact/dist/preact.min.js",
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat",
      "^react-addons-css-transition-group$": "preact-css-transition-group"
    }
  },
  "keywords": [
    "d3js",
    "chartjs",
    "preactjs"
  ],
  "description": "Components of interactive diagram by using preact"
}
